#include<bits/stdc++.h>
using namespace std;

void Solve()
{
    int n,m,x,y,d;cin>>n>>m>>x>>y>>d;

    if((x-d)<=1 && (x+d)>=n) cout<<-1<<endl;
    else if((y-d)<=1 && (y+d)>=m) cout<<-1<<endl;

    else if((x-d)<=1 && (y-d)<=1) cout<<-1<<endl;
    else if((x+d)>=n && (y+d)>=m) cout<<-1<<endl;
    else cout<<(m+n)-2<<endl;



}

int32_t main()
{
    int t;
    cin>>t;
    while(t--)
        Solve();
}
